  "description": "easy way to build your forms in laravel5",
  "license": "MIT",
  "keywords": [
    "laravel",
    "forms"
  ],
  "authors": [
    {
      "name": "Abdullah Ibn Farouk",
      "email": "eng.magwad@gmail.com"
    }
